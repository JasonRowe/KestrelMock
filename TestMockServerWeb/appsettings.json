{
  "MockSettings": [
    {
      "Request": {
        "Methods": [
          "GET"
        ],
        "PathStartsWith": "/starts/with"
      },
      "Response": {
        "Status": 200,
        "Headers": [
          {
            "Content-Type": "application/json"
          }
        ],
        "Body": "{\"hello\":\"world\"}"
      }
    },
    {
      "Request": {
        "Methods": [
          "GET"
        ],
        "PathMatchesRegex": "/\\d+"
      },
      "Response": {
        "Status": 200,
        "Headers": [
          {
            "Content-Type": "application/json"
          }
        ],
        "Body": null,
        "BodyFromFilePath": "./responses/response.jon"
      }
    },
    {
      "Request": {
        "Methods": [ "GET" ],
        "PathStartsWith": "/api/orders"
      },
      "Response": {
        "Status": 200,
        "Headers": [
          {
            "Content-Type": "application/json"
          }
        ],
        "Body": "{\"order\": \"ORDER-X\", \"product\": \"product-y\"}",
        "ReplaceDictionary": {
          "order": "orders/([\\w\\d]+)/.+",
          "product": "/([\\w\\d]+)$"
        }
      }
    }
  ]
}